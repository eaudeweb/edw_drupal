{#
/**
 * @file
 * Theme override to display the 'List item block' paragraph.
 *
 * @see ./modules/contrib/paragraphs/templates/paragraph.html.twig
 */
#}
{% set _layout = layout|default(1) %}

<div {{ attributes.addClass('row', 'row-cols-1', 'row-cols-md-' ~ layout) }}>
  {% for item in items %}
    <div class="col">
      {{ item }}
    </div>
  {% endfor %}
</div>
